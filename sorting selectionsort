function selectionSort(arr) {
    for (var i = 0 ; i < arr.length ; i++) {
        var lowest = i;
        for(var j = i + 1 ; j < arr.length  ; j++) {
           if(arr[j] < arr[lowest]) {
               lowest = j;
            } 
        }
        [arr[lowest],arr[i]] = [arr[i],arr[lowest]];
    
        
    }
    return arr;
}














console.log(selectionSort([5,4,1,3,2]));
