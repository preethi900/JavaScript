//Iterative way

function factorial(num) {
    let res = 1;
    for (var i = 1 ; i <= num ; i++) {
        if(num === 0 || num === 1) {
            return 1;
        } 
        res *= i;

    }
    return res;
} 
//Recursive way

function factorial(num) {
    
    if(num === 0 || num === 1) {
        return 1;
    }
      let fact = num*factorial(num - 1);
      num--;
      return fact;
}
