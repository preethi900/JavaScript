/**
 * @param {number[]} numbers
 * @param {number} target
 * @return {number[]}
 */
var twoSum = function(numbers, target) {
 let i = 0 ; j = numbers.length - 1;
 while (numbers[i] + numbers[j] !== target) {
     if (numbers[i] + numbers[j] < target) {
         i++;
     } else {
         j--;
     }
 }
    return [i+1,j+1];
};
