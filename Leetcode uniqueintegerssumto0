/**
 * @param {number} n
 * @return {number[]}
 */
var sumZero = function(n) {
    var arr = [];
    var num = -n +1;
    for (var i = 0 ; i < n ; i++) {
        arr.push(num);
        num = num + 2;
    }
    return arr;
};

/**
 * @param {number} n
 * @return {number[]}
 */
var sumZero = function(n) {
    let res = [];
    if(n % 2 === 1) {
        res.push(0);
    }
    for (var i = 1 ; i <= n /2 ; i++) {
        res.push(i,-i);
    }
    return res;
};
