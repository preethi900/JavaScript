var arraysIntersection = function(arr1, arr2, arr3) {
    var res = [];
    var res1 = [];
   for (var i = 0 ; i  < arr1.length ; i++) {
      for (var j = 0 ; j < arr2.length ; j++) {
          if (arr1[i] === arr2[j]) {
              res.push(arr1[i]);
          }
      } 
 }
     for (var i = 0 ; i  < arr3.length ; i++) {
      for (var j = 0 ; j < res.length ; j++) {
          if (arr3[i] === res[j]) {
              res1.push(arr3[i]);
          }
      } 
     }
    
    return res1;
};
