/**
 * @param {string} s
 * @param {string} t
 * @return {boolean}
 */
var isAnagram = function(s, t) {
    if (s.length !== t.length) {
        return false;
    }
    var sspl = s.split('');
    var tspl = t.split('');
    var sspl1 = sspl.sort().join('');
    var tspl1 = tspl.sort().join('');
    if (sspl1 === tspl1) {
        return true;
    } else {
        return false;
    }
};
