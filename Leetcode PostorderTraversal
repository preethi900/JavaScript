var postorderTraversal = function(root) {
    var visited = [];
    if(!root) {
        return visited;
    }
    traverse(root);
    function traverse(node) {
        if(node.left) {
            traverse(node.left);
        }
        if(node.right) {
            traverse(node.right);
        }
        visited.push(node.val)
    }
    return visited;
};
