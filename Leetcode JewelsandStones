/**
 * @param {string} J
 * @param {string} S
 * @return {number}
 */
var numJewelsInStones = function(J, S) {
    let j = J.split('');
    let s =S.split('');
    let count = 0
    for (var i =0 ; i < j.length; i++) {
        for (var k =0 ; k < s.length ; k++  ) {
            if (j[i] === s[k]) {
                count++;
            }    
        }
        
    }
    return count;
    
};
