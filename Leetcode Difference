/**
 * @param {string} s
 * @param {string} t
 * @return {character}
 */
var findTheDifference = function(s, t) {
   let s1 = s.split('').sort();
    let t1 = t.split('').sort();
    for (var i =0 ;i < t1.length; i++) {
        if (t1[i] != s1[i]) {
            return t1[i];
        }
    }
    
};
findTheDifference("abcd","abcde");
