/**
 * @param {number} N
 * @return {number}
 */
var fib = function(N) {
    if (N === 0 || N === 1) {
        return N;
    } else {
    var n1 = 0,n2 = 1,sum;
        for (var i =2 ; i <= N; i++) {
            var tmp = n2;
            n2 = n1;
            n1 = tmp + n1;
            sum = n1 + n2;
        }
        return sum;
    }
};

fib(9);
