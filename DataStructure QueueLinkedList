class Node {
    constructor(val) {
        this.val = val;
        this.next = null;
    }
}
class Stack {
    constructor() {
        this.first = null;
        this.last = null;
        this.size = 0;
    }
    push(val) {
        var newNode = new Node(val);
        if(!this.first) {
            this.first = newNode;
            this.last = this.first; 
        } else {
            let current = this.first;
            this.first = newNode;
            this.first.next = current;
            
        }
        this.size++;
        return this;
    }
