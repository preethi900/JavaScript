var validWordSquare = function(words) {
    for (var i = 0 ; i < words.length ; i++) {
        let res = '';
        for (var r= 0 ; r < words.length ; r++) {
            res += words[r][i] || '';
        }
        if(words[i] !== res) {
            return false;
        }
     
       }   
        return true;
};
